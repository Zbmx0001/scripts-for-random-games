for i = 1,99e+99 do
    while game:GetService("RunService").Stepped:Wait() do
        game:GetService("ReplicatedStorage"):WaitForChild("Remote_Events"):WaitForChild("Add_Jump"):FireServer(game.Players.LocalPlayer.General.Last_Add_Jump.Value + i)
    end
end

